{  
  "name": "木卫三 - 豆瓣",  
  "version": "1.1.1",  
  "manifest_version": 2,
  "description": "专注于豆瓣",  
  "background": { "scripts": ["background.js"] },
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["https://*.douban.com/*","http://*.douban.com/*"],
      "css": ["common.css"],
      "js": ["jquery.min.js","content_scripts.js"]
    }
  ],
   "page_action": {
        "default_icon": {
            "19": "movie_19.png",
            "38": "movie_38.png"
        },
        "default_title":"专注于豆瓣"
    }
} 